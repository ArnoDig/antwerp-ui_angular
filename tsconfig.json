{
  "compileOnSave": false,
  "compilerOptions": {
    "baseUrl": "./",
    "outDir": "./dist/out-tsc",
    "sourceMap": true,
    "declaration": false,
    "downlevelIteration": true,
    "experimentalDecorators": true,
    "module": "esnext",
    "moduleResolution": "node",
    "importHelpers": true,
    "target": "es2015",
    "typeRoots": [
      "node_modules/@types"
    ],
    "lib": [
      "es2018",
      "dom"
    ],
    "paths": {
      "ngx-layout": [
        "dist/ngx-layout"
      ],
      "ngx-layout/*": [
        "dist/ngx-layout/*"
      ],
      "ngx-logo": [
        "dist/ngx-logo"
      ],
      "ngx-logo/*": [
        "dist/ngx-logo/*"
      ],
      "aui-logo": [
        "dist/aui-logo"
      ],
      "aui-logo/*": [
        "dist/aui-logo/*"
      ],
      "aui-layout": [
        "dist/aui-layout"
      ],
      "aui-layout/*": [
        "dist/aui-layout/*"
      ],
      "aui-navigation-menu": [
        "dist/aui-navigation-menu"
      ],
      "aui-navigation-menu/*": [
        "dist/aui-navigation-menu/*"
      ],
      "aui-code-snippet": [
        "dist/aui-code-snippet"
      ],
      "aui-code-snippet/*": [
        "dist/aui-code-snippet/*"
      ],
      "analytics": [
        "dist/analytics"
      ],
      "analytics/*": [
        "dist/analytics/*"
      ],
      "aui-analytics": [
        "dist/aui-analytics"
      ],
      "aui-analytics/*": [
        "dist/aui-analytics/*"
      ],
      "aui-agenda": [
        "dist/aui-agenda"
      ],
      "aui-agenda/*": [
        "dist/aui-agenda/*"
      ],
      "aui-avatar": [
        "dist/aui-avatar"
      ],
      "aui-avatar/*": [
        "dist/aui-avatar/*"
      ],
      "aui-calendar": [
        "dist/aui-calendar"
      ],
      "aui-calendar/*": [
        "dist/aui-calendar/*"
      ],
      "aui-context": [
        "dist/aui-context"
      ],
      "aui-context/*": [
        "dist/aui-context/*"
      ],
      "aui-flyout": [
        "dist/aui-flyout"
      ],
      "aui-flyout/*": [
        "dist/aui-flyout/*"
      ],
      "aui-localstorage": [
        "dist/aui-localstorage"
      ],
      "aui-localstorage/*": [
        "dist/aui-localstorage/*"
      ],
      "aui-map": [
        "dist/aui-map"
      ],
      "aui-map/*": [
        "dist/aui-map/*"
      ],
      "aui-notifications": [
        "dist/aui-notifications"
      ],
      "aui-notifications/*": [
        "dist/aui-notifications/*"
      ],
      "aui-pagination": [
        "dist/aui-pagination"
      ],
      "aui-pagination/*": [
        "dist/aui-pagination/*"
      ],
      "aui-progress-bar": [
        "dist/aui-progress-bar"
      ],
      "aui-progress-bar/*": [
        "dist/aui-progress-bar/*"
      ],
      "aui-table": [
        "dist/aui-table"
      ],
      "aui-table/*": [
        "dist/aui-table/*"
      ],
      "aui-user-menu": [
        "dist/aui-user-menu"
      ],
      "aui-user-menu/*": [
        "dist/aui-user-menu/*"
      ],
      "aui-selectable-list": [
        "dist/aui-selectable-list"
      ],
      "aui-selectable-list/*": [
        "dist/aui-selectable-list/*"
      ],
      "aui-forms": [
        "dist/aui-forms"
      ],
      "aui-forms/*": [
        "dist/aui-forms/*"
      ],
      "ngx-utils": [
        "dist/ngx-utils"
      ],
      "ngx-utils/*": [
        "dist/ngx-utils/*"
      ]
    }
  },
  "angularCompilerOptions": {
    "fullTemplateTypeCheck": true,
    "strictInjectionParameters": true
  }
}
